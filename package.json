{
  "name": "daily-fashion",
  "version": "0.1.0",
  "description": "Daily Fashion site",
  "author": "Mike Polischuk",
  "scripts": {
    "dev": "nuxt",
    "production": "nuxt build",
    "now-build": "nuxt generate",
    "css-build": "node-sass --omit-source-map-url sass/mystyles.scss css/mystyles.css",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-watch",
    "heroku-postbuild": "npm install -g yarn && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mikemarsian/daily-fashion.git"
  },
  "keywords": [
    "css",
    "js",
    "html",
    "template",
    "ecommerce",
    "vuejs",
    "theme",
    "responsive",
    "nuxtjs"
  ],
  "license": "MIT",
  "dependencies": {
    "@nuxtjs/font-awesome": "^1.0.4",
    "@storefront-ui/vue": "^0.5.1",
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bulma": "^0.8.0",
    "nuxt": "^2.11.0",
    "vue": "^2.6.11",
    "vue-agile": "^1.0.11",
    "vue-carousel": "^0.18.0",
    "vue-product-carousel": "^1.2.0",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/preset-env": "~7.8.4",
    "@vue/cli-plugin-babel": "~4.2.2",
    "@vue/cli-service": "~4.2.2",
    "babel-plugin-transform-runtime": "~6.23.0",
    "node-sass": "^4.13.1",
    "sass-loader": "~8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": "plugin:vue/essential",
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
